{% extends "base.html.twig" %}

{% block title %}
    Créer une Catégorie
{% endblock %}

{% block body %}
    <h1>Créer une catégorie</h1>

    {{ form_start(formView) }}
    {{ form_errors(formView) }}
    {{ form_widget(formView) }}

    <button type="submit" class="btn btn-primary">
        <i class="fas fa-save fa-fw"></i>
        Créer la catégorie
    </button>

    {{ form_end(formView) }}


{% endblock %}