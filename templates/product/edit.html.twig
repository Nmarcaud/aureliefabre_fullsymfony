{% extends "base.html.twig" %}

{% block title %}
	Modifier un produit
{% endblock %}

{% block body %}
	<h1>Nouveau produit</h1>

	{{ form_start(formView) }}
    {{ form_errors(formView) }}
    {{ form_widget(formView) }}

    <button type="submit" class="btn btn-primary">
        <i class="fas fa-save fa-fw"></i>
        Modifier le produit
    </button>

    {{ form_end(formView) }}


{% endblock %}
